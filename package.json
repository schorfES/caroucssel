{
  "name": "caroucssel",
  "version": "0.9.0",
  "description": "A lightweight dependency-free css carousel.",
  "main": "dist/caroucssel.umd.js",
  "module": "dist/caroucssel.esm.js",
  "types": "dist/caroucssel.d.ts",
  "scripts": {
    "validate": "make validate",
    "test": "snyk test && make tests",
    "coverage": "make coverage",
    "web": "make web",
    "build": "make build"
  },
  "watch": {
    "web": {
      "patterns": [
        "web",
        "dist"
      ],
      "extensions": "scss",
      "delay": 10000
    },
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schorfES/caroucssel.git"
  },
  "keywords": [],
  "author": "Norman Rusch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schorfES/caroucssel/issues"
  },
  "homepage": "https://schorfes.github.io/caroucssel/",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-modules-umd": "^7.8.3",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "audit-ci": "^4.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "codecov": "^3.6.1",
    "eslint": "^7.3.1",
    "eslint-plugin-jest": "^24.0.2",
    "gh-pages": "^3.2.3",
    "jest": "^27.0.4",
    "live-server": "^1.2.1",
    "node-sass": "^6.0.0",
    "np": "^7.2.0",
    "npm-watch": "^0.9.0",
    "rollup": "^2.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "sass-lint": "^1.13.1",
    "snyk": "^1.518.0",
    "uglify-js": "^3.7.5"
  }
}
