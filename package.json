{
  "name": "caroucssel",
  "version": "0.10.2",
  "description": "A lightweight dependency-free css carousel.",
  "main": "dist/caroucssel.umd.js",
  "module": "dist/caroucssel.esm.js",
  "types": "dist/caroucssel.d.ts",
  "scripts": {
    "validate": "make validate",
    "test": "make tests",
    "web": "make web",
    "build": "make build"
  },
  "watch": {
    "web": {
      "patterns": [
        "web",
        "dist"
      ],
      "extensions": "scss",
      "delay": 10000
    },
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schorfES/caroucssel.git"
  },
  "keywords": [],
  "author": "Norman Rusch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schorfES/caroucssel/issues"
  },
  "homepage": "https://schorfes.github.io/caroucssel/",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.4.10",
    "audit-ci": "^4.1.0",
    "eslint": "^7.29.0",
    "eslint-plugin-jest": "^24.0.2",
    "gh-pages": "^3.2.3",
    "jest": "^27.1.1",
    "live-server": "^1.2.1",
    "np": "^7.5.0",
    "npm-watch": "^0.11.0",
    "rollup": "^2.52.2",
    "rollup-plugin-replace": "^2.2.0",
    "sass": "^1.35.1",
    "sass-lint": "^1.13.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "uglify-js": "^3.13.9"
  }
}
